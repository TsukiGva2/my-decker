{script:home.2}
on tty text dialogue speed startdialogue pipsound do
 if startdialogue=0
  dialogue.text:""
 else
  dialogue.text:startdialogue
 end
 each x i in text
  dialogue.text:"" fuse (dialogue.text,x)
  dialogue.scrollto[i]
  play[pipsound]
  sleep[speed]
 end
end

